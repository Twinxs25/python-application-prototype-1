MDScreen:
    name: "menu"
    MDBoxLayout:

        orientation: "vertical"

        MDToolbar:
            pos_hint: {"top" : 1}
            left_action_items: [["menu", lambda x: app.callback(x)]]
            right_action_items: [["timer", lambda x: app.time()]]



        MDTextFieldRound:
            pos_hint: {'center_x' : 0.5, 'center_y' : 0.30}
            hint_text: 'Enter Website URL'
            size_hint_x: .8
            halign: "center"
            
            
        
        Image:
            source: "name.png"
            size: 3, 3
            halign: "center"
            
        MDBottomAppBar:

            MDToolbar:
                title: "Title"
                icon: "language-python"
                type: "bottom"
                left_action_items: [["menu", lambda x: x]]
                mode: "free-end"